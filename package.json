{
  "name": "lesswrong",
  "version": "2.1.0",
  "scripts": {
    "checktypes": "scripts/lint_and_checktypes.sh",
    "lint": "scripts/lint_and_checktypes.sh",
    "acceptmigrations": "tsx scripts/acceptMigrations.ts || git diff --no-index schema/accepted_schema.sql schema/schema_to_accept.sql",
    "makemigrations": "tsx scripts/makeMigrations.ts",
    "eslint": "eslint --max-warnings 0 --cache --ext .jsx,js,tsx,ts packages",
    "build": "tsx ./build.ts",
    "start": "pnpm build --run",
    "prod": "NODE_ENV=production pnpm start",
    "production": "pnpm prod",
    "migrate": "tsx ./migrate.ts",
    "branchdb": "tsx scripts/branchDb.ts",
    "dev": "pnpm build --watch --run",
    "start-dev-db": "pnpm build --no-clear -run --watch --lint --db ../LessWrong-Credentials/connectionConfigs/dev.json --settings ../LessWrong-Credentials/settings-local-dev-devdb.json",
    "start-prod-db": "pnpm build --no-clear -run --watch --lint --db ../LessWrong-Credentials/connectionConfigs/prod.json --settings ../LessWrong-Credentials/settings-local-dev-proddb.json",
    "lw-start-local-db": "pnpm build --no-clear -run --watch --lint --db ../LessWrong-Credentials/connectionConfigs/local.json --settings settings.json",
    "shell": "pnpm build --no-clear -run --settings settings.json --shell",
    "shell-dev-db": "pnpm build --no-clear -run --db ../LessWrong-Credentials/connectionConfigs/dev.json --settings ../LessWrong-Credentials/settings-local-dev-devdb.json --shell",
    "shell-prod-db": "pnpm build --no-clear -run --db ../LessWrong-Credentials/connectionConfigs/prod.json --settings ../LessWrong-Credentials/settings-local-dev-proddb.json --shell",
    "start-af": "pnpm build --no-clear -run --watch --lint --settings settings-af.json",
    "start-af-dev-db": "pnpm build --no-clear -run --watch --lint --db ../LessWrong-Credentials/connectionConfigs/dev.json --settings ../LessWrong-Credentials/settings-local-dev-af-devdb.json",
    "start-af-prod-db": "pnpm build --no-clear -run --watch --lint --db ../LessWrong-Credentials/connectionConfigs/prod.json --settings ../LessWrong-Credentials/settings-local-dev-af-proddb.json",
    "unit": "jest --config jest.unit.config.ts --forceExit",
    "unit-ci": "jest --config jest.unit.config.ts --forceExit --ci",
    "unit-watch": "jest --config jest.unit.config.ts --watchAll",
    "unit-coverage": "jest --config jest.unit.config.ts --forceExit --coverage",
    "exec-accepted-schema": "psql $PG_URL -f ./schema/accepted_schema.sql",
    "create-integration-db": "PG_URL=$(cat ../ForumCredentials/testing-pg-conn.txt) ./scripts/createIntegrationPgDb.sh",
    "create-integration-db-ci": "pnpm exec-accepted-schema && ./scripts/createIntegrationPgDb.sh",
    "integration": "PG_URL=$(cat ../ForumCredentials/testing-pg-conn.txt) PG_MAX_CONNECTIONS=5 jest --config jest.integration.config.ts --forceExit",
    "integration-ci": "PG_MAX_CONNECTIONS=5 jest --config jest.integration.config.ts --forceExit --ci",
    "integration-watch": "jest --config jest.integration.config.ts --watchAll",
    "integration-coverage": "jest --config jest.integration.config.ts --forceExit --coverage",
    "combine-coverage": "./scripts/combineCoverage.sh",
    "encrypt-credentials": "tar czf credentials.tar.gz --exclude '.git/*' -C ../ ./LessWrong-Credentials; travis encrypt-file credentials.tar.gz --add; rm credentials.tar.gz",
    "generate": "scripts/generateTypes.sh",
    "rebuild-ckeditor": "cd ./public/lesswrong-editor && pnpm install && pnpm run build",
    "deploy": "scripts/deploy.sh",
    "command": "pnpm build -run --command",
    "elasticsearch-export": "pnpm command 'Globals.elasticConfigureIndexes()'; pnpm command 'Globals.elasticExportAll()'",
    "ea-command-dev": "pnpm build -run --settings ../ForumCredentials/settings-dev.json --postgresUrlFile ../ForumCredentials/dev-pg-conn.txt --command",
    "ea-command-staging-db": "pnpm build -run --settings ../ForumCredentials/settings-dev.json --postgresUrlFile ../ForumCredentials/dev-pg-conn.txt --command",
    "ea-command-prod-db": "pnpm build -run --settings ../ForumCredentials/settings-dev.json --postgresUrlFile ../ForumCredentials/dev-pg-conn.txt --command",
    "ea-command-testing": "pnpm build -run --settings ../ForumCredentials/settings-dev.json --postgresUrlFile ../ForumCredentials/testing-pg-conn.txt --command",
    "ea-command-ci": "pnpm build -run --settings ./settings-test.json --postgresUrl $PG_URL --command",
    "ea-start": "pnpm build -run -watch --lint --settings ../ForumCredentials/settings-dev.json --postgresUrlFile ../ForumCredentials/dev-pg-conn.txt",
    "ea-start-xpost-dev": "PORT=4000 pnpm build -run -watch --lint --settings ../ForumCredentials/settings-xpost.json --postgresUrlFile ../ForumCredentials/xpost-pg-conn.txt",
    "ea-start-staging-db": "pnpm build -run --watch --lint --settings ../ForumCredentials/settings-local-dev-staging-db.json --postgresUrlFile ../ForumCredentials/staging-pg-conn.txt",
    "ea-start-prod-db": "pnpm build -run --watch --lint --settings ../ForumCredentials/settings-local-dev-prod-db.json --postgresUrlFile ../ForumCredentials/prod-pg-conn.txt",
    "ea-start-testing-db": "pnpm build -run --watch --settings ./settings-test.json --postgresUrl ${PG_URL:=$(cat ../ForumCredentials/testing-pg-conn.txt)}",
    "init-cypress-db": "pnpm ea-command-testing 'Globals.dropAndCreatePg({templateId: \"cypress_template\", seed: true, dropExisting: true})'",
    "init-cypress-db-ci": "pnpm exec-accepted-schema && ./scripts/timeout.sh 120 pnpm ea-command-ci 'Globals.dropAndCreatePg({templateId: \"cypress_template\", seed: true, dropExisting: true})'",
    "cypress-run-ci": "pnpm wait-on tcp:3000 && pnpm ea-cypress-run",
    "ea-cypress-run": "pnpm cypress run --env TESTING_DB_URL=${TESTING_DB_URL:=$(cat ../ForumCredentials/testing-db-conn.txt)},PG_URL=${PG_URL:=$(cat ../ForumCredentials/testing-pg-conn.txt)}",
    "ea-cypress-open": "pnpm cypress open --env TESTING_DB_URL=${TESTING_DB_URL:=$(cat ../ForumCredentials/testing-db-conn.txt)},PG_URL=${PG_URL:=$(cat ../ForumCredentials/testing-pg-conn.txt)}",
    "lw-cypress-run": "pnpm cypress run --env TESTING_DB_URL=${TESTING_DB_URL:=$(cat ../ForumCredentials/testing-db-conn.txt)}}",
    "lw-cypress-open": "pnpm cypress open --env TESTING_DB_URL=${TESTING_DB_URL:=$(cat ../ForumCredentials/testing-db-conn.txt)}}",
    "ea-pg": "psql $(< ../ForumCredentials/dev-pg-conn.txt)",
    "ea-pg-xpost-dev": "psql $(< ../ForumCredentials/xpost-pg-conn.txt)",
    "ea-pg-staging": "psql $(< ../ForumCredentials/staging-pg-conn.txt)",
    "ea-pg-testing": "psql $(< ../ForumCredentials/testing-pg-conn.txt)",
    "ea-pg-prod": "psql $(< ../ForumCredentials/prod-pg-conn.txt)",
    "ea-pg-dev-admin": "psql $(< ../ForumCredentials/dev-staging-admin-pg-conn.txt)",
    "ea-pg-prod-admin": "psql $(< ../ForumCredentials/prod-admin-pg-conn.txt)",
    "ea-load-dev-db": "tsx ./scripts/loadDbSettings.ts dev",
    "ea-load-xpost-dev-db": "tsx ./scripts/loadDbSettings.ts xpost",
    "ea-load-staging-db": "tsx ./scripts/loadDbSettings.ts staging",
    "ea-load-prod-db": "node ../ForumCredentials/assertSafeToUpdateDbSettings.js && tsx ./scripts/loadDbSettings.ts prod",
    "ea-load-testing-db": "tsx ./scripts/loadDbSettings.ts testing",
    "ea-replicate-dev-db": "./scripts/eaReplicateDevDb.sh",
    "local-cypress-run": "pnpm cypress run",
    "local-cypress-open": "pnpm cypress open",
    "brigader-check": "tsx scripts/brigaderCheck.ts"
  },
